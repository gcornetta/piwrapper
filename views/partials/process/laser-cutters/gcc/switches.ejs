               <div class="row">								
		  <div class="col-md-3">
	             <legend><%= laserPanel.gcc.switches.title %></legend>
		     <div class="col-md-6">
			<p class="category"><%= laserPanel.gcc.switches.autofocus %></p>
	                <div class="switch" data-on-label="<i class='fa fa-check'></i>" data-off-label="<i class='fa fa-times'></i>">
			   <input type="checkbox" name="switchAutofocus" checked />
			</div>
                        <div id="switch-div-cut" style="display: none;"> 
			<p class="category"><%= laserPanel.gcc.switches.sort %></p>
			<div class="switch" data-on-label="<i class='fa fa-check'></i>" data-off-label="<i class='fa fa-times'></i>">
		           <input type="checkbox" name="switchSort"checked />
			</div>
                        </div>
                        <div id="switch-div-halftone" style="display: none;">
                        <p class="category"><%= laserPanel.gcc.switches.sort %></p>
			<div class="switch" data-on-label="<i class='fa fa-check'></i>" data-off-label="<i class='fa fa-times'></i>">
		           <input type="checkbox" name="switchSort"checked />
			</div>
			<p class="category"><%= laserPanel.gcc.switches.fill %></p>
			<div class="switch" data-on-label="<i class='fa fa-check'></i>" data-off-label="<i class='fa fa-times'></i>">
		           <input type="checkbox" name="switchFill"checked />
			</div>
                        </div>
	             </div>
		  </div>
		  <div class="col-md-4">
		     <legend><%= laserPanel.gcc.position.title %></legend>
		     <div class="row">
		        <fieldset>
			   <div class="form-group">
			      <div class="col-sm-6">
			         <div class="row">
			            <label class="col-sm-2 control-label">X:</label>
				    <div class="col-sm-8">
				       <input type="text" name="xCoord" placeholder="50" value="50" class="form-control">
				    </div>
				  </div>
				  <br />
				  <div class="row">
				  <label class="col-sm-2 control-label">Y:</label>
			          <div class="col-sm-8">
			             <input type="text" name="yCoord" placeholder="50" value="50" class="form-control">
				  </div>
			       </div>
			    </div>
			    <div class="col-sm-6">
			       <label class="radio">
			          <input type="radio" data-toggle="radio" name="origin" value="<%= laserPanel.gcc.position.topleft %>" checked> <%= laserPanel.gcc.position.topleft %>
			       </label>
			       <label class="radio">
			          <input type="radio" data-toggle="radio" name="origin" value="<%= laserPanel.gcc.position.topleft %>"> <%= laserPanel.gcc.position.topright %>
			       </label>
			       <label class="radio">
			          <input type="radio" data-toggle="radio" name="origin" value="<%= laserPanel.gcc.position.topleft %>"> <%= laserPanel.gcc.position.bottomleft %>
			       </label>
			       <label class="radio">
			          <input type="radio" data-toggle="radio" name="origin" value="<%= laserPanel.gcc.position.topleft %>"> <%= laserPanel.gcc.position.bottomright %>
			       </label>
			    </div>
			 </div>
	              </fieldset>
		   </div>
		</div>
		<div class="col-md-4 col-md-offset-1">
		   <legend><%= laserPanel.gcc.select.process.title %></legend>
	           <div class="row">
	              <fieldset>
		         <div class="form-group">
			    <div class="col-sm-12">
		               <select id="processes-gcc" name="process" class="selectpicker" data-title="Process" data-style="btn-danger btn-block" data-menu-style="dropdown-blue" onchange="showSwitchGcc(this)">
			          <option value="cut"><%= laserPanel.gcc.select.process.cut %></option>
			          <option value="halftone"><%= laserPanel.gcc.select.process.halftone %></option>
			       </select>
			    </div>
			 </div>
	              </fieldset>			
		   </div>
                   <br />
		   <h4 class="title"><%= laserPanel.gcc.select.material.title %></h4>	 
                   <div class="row">
	              <fieldset>
		         <div class="form-group">
			    <div class="col-sm-12">
		               <select name="material" class="selectpicker" data-title="Material" data-style="btn-danger btn-block" data-menu-style="dropdown-blue">
			          <option value="cardboard"><%= laserPanel.gcc.select.material.cardboard %></option>
			          <option value="acrylic"><%= laserPanel.gcc.select.material.acrylic %></option>
				  <option value="wood"><%= laserPanel.gcc.select.material.wood %></option>
				  <option value="mylar"><%= laserPanel.gcc.select.material.mylar %></option>
			       </select>
			    </div>
			 </div>
		      </fieldset>			
	           </div>
                   <br />
		</div>
             </div>
