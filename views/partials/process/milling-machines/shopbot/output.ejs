        <label class="col-sm-2 control-label">cut speed (mm/s)</label>
        <div class="col-sm-2">
          <div class="input-group">
            <input type="number" step=1 name="cut_speed" placeholder="20" value="20" class="form-control">
          </div>
        </div>
        <label class="col-sm-2 control-label">plunge speed (mm/s)</label>
        <div class="col-sm-2">
          <div class="input-group">
            <input type="number" step=1 name="plunge_speed" placeholder="20" value="20" class="form-control">
          </div>
        </div>
        <label class="col-sm-2 control-label">jog speed (mm/s)</label>
        <div class="col-sm-2">
          <div class="input-group">
