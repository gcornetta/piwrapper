        <label class="col-sm-2 control-label">jog height (mm)</label>
        <div class="col-sm-2">
          <div class="input-group">
            <input type="number" step=1 name="jog_height" placeholder="5" value="5" class="form-control">
          </div>
        </div>
        <label class="col-sm-2 control-label">spindle speed (RPM)</label>
        <div class="col-sm-2">
          <div class="input-group">
            <input type="number" step=1 name="spindle_speed" placeholder="10000" value="10000" class="form-control">
