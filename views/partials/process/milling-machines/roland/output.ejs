             <div class="row">
	        <div class="col-md-9">
		   <fieldset>
	              <legend><%= millingPanel.roland.engine.title %></legend>
		         <div class="form-group">
			    <label class="col-sm-1 control-label"><%= millingPanel.roland.engine.power %></label>
		            <div class="col-sm-2">
			       <div class="input-group">
			          <input type="text" name="power" placeholder="25" value="25" class="form-control">
			       </div>
			    </div>
			    <label class="col-sm-1 control-label"><%= millingPanel.roland.engine.speed %></label>
			    <div class="col-sm-2">
			       <div class="input-group">
			          <input type="text" name="speed" placeholder="75" value="75" class="form-control">
			       </div>
			    </div>
		            <label class="col-sm-1 control-label"><%= millingPanel.roland.engine.rate %></label>
		            <div class="col-sm-2">
			       <div class="input-group">
			          <input type="text" name="rate" placeholder="500" value="500" class="form-control">
			       </div>
			    </div>
			 </div>
		      </fieldset>
		   </div>
		<div class="col-md-3">
		   <fieldset>
		      <legend><%= millingPanel.roland.select.machine.title %></legend>
		         <div class="form-group">				 
                            <select id="machines" name="machines" class="selectpicker" data-title="Machine" data-style="btn-danger btn-block" data-menu-style="dropdown-blue">
                               <option value="mdx_15"><%= millingPanel.roland.select.machine.mdx15 %></option>
                               <option value="mdx_20"><%= millingPanel.roland.select.machine.mdx20 %></option>
                               <option value="mdx_40"><%= millingPanel.roland.select.machine.mdx40 %></option>
                               <option value="srm_20"><%= millingPanel.roland.select.machine.srm20 %></option>
			    </select>
                         </div>
		   </fieldset>
		</div>
	      </div>